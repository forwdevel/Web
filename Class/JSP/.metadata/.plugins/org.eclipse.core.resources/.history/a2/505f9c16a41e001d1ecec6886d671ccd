<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>웹소켓 채팅</title>
</head>
<body>
	<script>
	function chatWinOpen() {
		var id = document.getElementById("charId");
		if(id.value == "") {
			alert("대화명을 입력 후 채팅창을 열어주세요.");
			id.focus();
			return;
		}
		window.open("ChatWindow.jso?chatId=" + id.value, "", "width=320,height=400");
		id.value = "";
	}
	</script>
</body>
</html>